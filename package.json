{
  "name": "koa-graphql-starter",
  "version": "0.0.1",
  "description": "GraphQL project boilerplate based on Koa.js, TypeScript and Apollo Server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "start:dev": "nodemon",
    "generate": "graphql-codegen --watch --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrd-fe/koa-graphql-starter.git"
  },
  "keywords": [
    "GraphQL",
    "TypeScript",
    "Apollo-Server",
    "Koa.js"
  ],
  "author": "RRD-FE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rrd-fe/koa-graphql-starter/issues"
  },
  "homepage": "https://github.com/rrd-fe/koa-graphql-starter#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-resolvers": "1.7.0",
    "@types/koa": "^2.0.49",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-koa": "^2.9.3",
    "graphql": "^14.5.4",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "merge-graphql-schemas": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
